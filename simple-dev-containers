}

# :command.parse_requirements
simple_dev_containers_update_parse_requirements() {
	# :command.fixed_flags_filter
	while [[ $# -gt 0 ]]; do
		case "${1:-}" in
			--help | -h)
				long_usage=yes
				simple_dev_containers_update_usage
				exit
				;;

			*)
				break
				;;

		esac
	done

	# :command.command_filter
	action="update"

	# :command.parse_requirements_while
	while [[ $# -gt 0 ]]; do
		key="$1"
		case "$key" in

			-?*)
				printf "invalid option: %s\n" "$key" >&2
				exit 1
				;;

			*)
				# :command.parse_requirements_case
				# :command.parse_requirements_case_simple
				printf "invalid argument: %s\n" "$key" >&2
				exit 1

				;;

		esac
	done

}

# :command.initialize
initialize() {
	version="0.1.0"
	long_usage=''
	set -e

	# src/initialize.sh

	CURRENT_USER="$(whoami)"
	CURRENT_UID="$(id -u)"
	SCRIPT_PATH="$HOME/.local/bin/simple-dev-containers"

}

# :command.run
run() {
	declare -A args=()
	declare -A deps=()
	declare -a other_args=()
	declare -a env_var_names=()
	declare -a input=()
	normalize_input "$@"
	parse_requirements "${input[@]}"

	case "$action" in
		"completions") simple_dev_containers_completions_command ;;
		"create") simple_dev_containers_create_command ;;
		"list") simple_dev_containers_list_command ;;
		"run") simple_dev_containers_run_command ;;
		"remove") simple_dev_containers_remove_command ;;
		"uninstall") simple_dev_containers_uninstall_command ;;
		"update") simple_dev_containers_update_command ;;
	esac
}

initialize
run "$@"
