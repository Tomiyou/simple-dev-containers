# :command.usage
simple_dev_containers_usage() {
	printf "simple-dev-containers - Script for easily creating/running/cleaning docker containers\n\n"

	printf "%s\n" "Usage:"
	printf "  simple-dev-containers COMMAND\n"
	printf "  simple-dev-containers [COMMAND] --help | -h\n"
	printf "  simple-dev-containers --version | -v\n"
	echo
	# :command.usage_commands
	printf "%s\n" "Commands:"
	printf "  %s   Generate bash completions\n" "completions"
	printf "  %s   Create a docker container\n" "create     "
	printf "  %s   List available containers\n" "list       "
	printf "  %s   Run a created docker container interactively\n" "run        "
	printf "  %s   Remove a created docker container\n" "remove     "
	printf "  %s   Uninstall this script\n" "uninstall  "
	printf "  %s   Update this script\n" "update     "
	echo

	# :command.long_usage
	if [[ -n $long_usage ]]; then
		printf "%s\n" "Options:"

		# :command.usage_fixed_flags
		printf "  %s\n" "--help, -h"
		printf "    Show this help\n"
		echo
		printf "  %s\n" "--version, -v"
		printf "    Show version number\n"
		echo

	fi
}

